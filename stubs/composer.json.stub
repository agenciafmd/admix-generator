{
    "name": ":packageVendor:/local-:packageName:",
    "description": ":packageDescription:",
    "authors": [
        {
            "name": ":authorName:",
            "email": ":authorEmail:",
            "homepage": "https://fmd.ag/",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "10.*"
    },
    "autoload": {
        "psr-4": {
            ":namespaceVendor:\\:namespaceName:\\": "src/",
            ":namespaceVendor:\\:namespaceName:\\Database\\Factories\\": "database/factories/",
            ":namespaceVendor:\\:namespaceName:\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                ":namespaceVendor:\\:namespaceName:\\Providers\\:className:ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
